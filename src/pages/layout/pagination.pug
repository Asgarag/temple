- var pagination = [{number: '1', flag: 0}, {number: '2', flag: 1}, {number: '3', flag: 1}, {number: '4', flag: 1}, {number: '5', flag: 1}, {number: '6', flag: 1}, {number: '...', flag: 1}, {number: '10', flag: 1}]

nav.pagination
	ul.list.pagination__list
		for elem in pagination
			li.pagination__list-item
				if elem.flag === 1
					a.link.pagination__link(href="#") #{elem.number}
				else
					span.pagination__link-active #{elem.number}